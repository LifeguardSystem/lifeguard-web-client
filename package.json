{
  "name": "lifeguard-web-client",
  "version": "0.0.0",
  "description": "A simple Lifeguard web client",
  "main": "/_site/index.html",
  "type": "module",
  "scripts": {
    "build": "webpack && npx @11ty/eleventy",
    "start": "npm-run-all pages -p watch:*",
    "pages": "webpack",
    "watch:pages": "webpack --watch",
    "watch:11ty": "npx @11ty/eleventy --serve --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "author": "",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/LifeguardSystem/lifeguard-web-client/issues"
  },
  "homepage": "https://github.com/LifeguardSystem/lifeguard-web-client#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "glob": "^10.3.3",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
